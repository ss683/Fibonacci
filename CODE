package com.company;
import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
        int number1 = 0;
        int number2 = number1 + 1;
        int number3 = number1 + number2;
        int number4 = number3 + number2;
        int number5 = number4 + number3;
        int number6 = number5 + number4;
        int number7 = number6 + number5; //probably can do a loop
        int number8 = number7 + number6;
        int number9 = number8 + number7;
        int number10 = number9 + number8;
        System.out.print("First ten numbers of Fibonacci Sequence." + " ");
        System.out.println(number1 + " " + number2 + " " + number3 + " " + number4 + " " + number5 + " " + number6 + " " + number7 + " " + number8 + " " + number9 + " " + number10);


